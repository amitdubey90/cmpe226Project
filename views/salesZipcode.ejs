<html>
  <head>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
      google.load("visualization", "1.1", {packages:["bar"]});
      google.setOnLoadCallback(drawChart);
      function drawChart() {



        var data = google.visualization.arrayToDataTable([
          ['Zip Codes', 'Sales',],
          ['<%- data[0].CustomerZipCode %>', <%- data[0].sales %>],
          ['<%- data[1].CustomerZipCode %>', <%- data[1].sales %>],
          ['<%- data[2].CustomerZipCode %>', <%- data[2].sales %>],
          ['<%- data[3].CustomerZipCode %>', <%- data[3].sales %>],
          ['<%- data[4].CustomerZipCode %>', <%- data[4].sales %>]

        ]);

        var options = {
          chart: {
            title: '',
            subtitle: 'Sales',
          },
          bars: 'horizontal' // Required for Material Bar Charts.
        };

        var chart = new google.charts.Bar(document.getElementById('barchart_material'));

        chart.draw(data, options);
      }



    </script>
  </head>
  <body>
  <h1> Top 5  company sales performance as per zip code </h1>
    <div id="barchart_material" style="width: 900px; height: 500px;"></div>
  </body>
</html>