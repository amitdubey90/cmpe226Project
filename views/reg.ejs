<!DOCTYPE HTML>
<html>
<head>
    <title>Node App</title>
    <meta charset="utf-8">
    <link rel="stylesheet" href="/css/bootstrap.css">
    <link rel="stylesheet" href="/css/bootstrap-responsive.css">
    <link rel="stylesheet" href="/css/site.css">
</head>
<body>

<div class="container">

    <div class="hero-unit reg=header">
        <h4>Sign Up</h4>
    </div>

    <div id="regForm">
        <form action="/user/create" method="POST" class="form-horizontal">
           <div class="control-group">
                <label class="control-label" for="username_input">Firstname</label>

                <div class="controls">
                    <input type="text" id="username_input" name="username">
                </div>
            </div>

            <div class="control-group">
                <label class="control-label" for="userlastname_input">Lastname</label>

                <div class="controls">
                    <input type="text" id="userlastname_input" name="userlastname">
                </div>
            </div>


              <div class="control-group">
                <label class="control-label" for="userphone_input">Phone</label>

                <div class="controls">
                    <input type="text" id="userphone_input" name="userphone">
                </div>
            </div>

          
            <div class="control-group">
                <label class="control-label" for="userstreet_input">Street</label>

                <div class="controls">
                    <input type="text" id="userstreet_input" name="userstreet">
                </div>
            </div>


            <div class="control-group">
                <label class="control-label" for="usercity_input">City</label>

                <div class="controls">
                    <input type="text" id="usercity_input" name="usercity">
                </div>
            </div>

            <div class="control-group">
                <label class="control-label" for="userzipcode_input">Zipcode</label>

                <div class="controls">
                    <input type="text" id="userzipcode_input" name="userzipcode">
                </div>
            </div>

             <div class="control-group">
                <label class="control-label" for="userstate_input">State</label>

                <div class="controls">
                    <input type="text" id="userstate_input" name="userstate">
                </div>
            </div>


           <div class="control-group">
                <label class="control-label" for="usercountry_input">Country</label>

                <div class="controls">
                    <input type="text" id="usercountry_input" name="usercountry">
                </div>
            </div>



            <div class="control-group">
                <label for="email_input" class="control-label" for="email_input">Email</label>

                <div class="controls">
                    <input type="text" id="email_input" name="email">
                </div>
            </div>

            <div class="control-group">
                <label for="password_input" for="password_input" class="control-label">Password</label>

                <div class="controls">
                    <input type="password" id="password_input" name="password">
                </div>
            </div>            
            
            <div class="control-group">
                <div class="controls">
                    <input type="submit" name="Submit" value="Register" class="btn">
                </div>
            </div>
        </form>
    </div>
</div>
<script type="text/javascript" src="/js/jquery.js"></script>
<script type="text/javascript" src="/js/bootstrap.min.js"></script>
</body>
</html>